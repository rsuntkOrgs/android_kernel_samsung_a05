/* CAMERA EEPROM */
&i2c4 {
	mtk_camera_eeprom1:camera_eeprom1@51 {
		compatible = "mediatek,camera_eeprom";
		reg = <0x51>;
		status = "okay";
	};
	mtk_camera_eeprom4:camera_eeprom4@50 {
		compatible = "mediatek,camera_eeprom";
		reg = <0x50>;
		status = "okay";
	};
};
&i2c2 {
	mtk_camera_eeprom0:camera_eeprom0@59 {
		compatible = "mediatek,camera_eeprom";
		reg = <0x59>;
		status = "okay";
	};
};
/* CAMERA EEPROM end */

